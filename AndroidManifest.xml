<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rozzles.camera"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="13"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.rozzles.camera.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.rozzles.camera.ShutterRelease"
            android:label="@string/title_activity_shutter_release" >
        </activity>
        <activity
            android:name="com.rozzles.camera.Timelapse"
            android:label="@string/title_activity_timelapse" >
        </activity>
        <activity
            android:name="com.rozzles.camera.LightTrigger"
            android:label="@string/title_activity_light_trigger" >
        </activity>
        <activity
            android:name="com.rozzles.camera.SoundTrigger"
            android:label="@string/title_activity_sound_trigger" >
        </activity>
        <activity
            android:name="com.rozzles.camera.DevLink"
            android:label="@string/title_activity_dev_link" >
        </activity>
    </application>

</manifest>
